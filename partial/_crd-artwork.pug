//- Gallery artwork figures

//- Artwork figure mixin – classes referenced by lightGallery.js
	Declare:
	Piece name,
	Full-size image source address,
	Thumbnail image source address,
	Thumbnails image alt text,
	Piece year created,
	Piece medium,
	Piece material,
	Piece width in inches,
	Piece height inches,
	Image aspect ratio class,
	Name of series piece belongs to,
	Series link address,
	List of tags relavent to piece
mixin galleryItem(pieceName, imageFull, imageThumb, imageAlt, pieceYear, pieceMedium, pieceMaterial, pieceWidth, pieceHeight, aspectRatio, seriesName, seriesLink, ...tagList)
	figure.ap-c-crd-artwork(
		data-src="https://res.cloudinary.com/adamska/image/upload/f_auto,q_auto:best/v1/work/grazyna-adamska-jarecka_" + imageFull
		data-sub-html=".ap-c-crd-artwork__info"
	)
		div.ap-c-crd-artwork__thumb(
			class="ap-l-ratio--" + aspectRatio
			style="background-image: url(https://res.cloudinary.com/adamska/image/upload/c_scale,e_blur:512,f_auto,q_auto:good,w_64/v1/work/grazyna-adamska-jarecka_" + imageThumb + ");"
		)
			img.ap-c-crd-artwork__image(
				src="https://res.cloudinary.com/adamska/image/upload/f_auto,q_auto:best/v1/work/grazyna-adamska-jarecka_" + imageThumb
				alt=imageAlt + " by Grazyna Adamska Jarecka"
			)
			div.ap-c-crd-artwork__icon
				+icon(iconExpand)
		figcaption.ap-c-crd-artwork__info
			div.ap-c-crd-artwork__title
				h4.ap-c-crd-artwork__piece=pieceName
				span.ap-c-crd-artwork__series
					span &ensp;&mdash;&ensp;in&nbsp;
					a(href=seriesLink)=seriesName
			div.ap-c-crd-artwork__spec
				a(
					href=urlRoot + pieceYear  
					title="More artworks created in" + pieceYear
				)=pieceYear
				span &nbsp;&mdash;&nbsp;
				a(
					href=urlRoot + pieceMedium
					title="More artworks made with" + pieceMedium
				)=pieceMedium
				span &nbsp;on&nbsp;
				a(
					href=urlRoot + pieceMaterial
					title="More artworks made with" + pieceMaterial
				)=pieceMaterial
				span &comma;&nbsp;
				span=pieceWidth
					| &nbsp;&times;&nbsp;
				span=pieceHeight
					| &nbsp;in
			ul.ap-c-crd-artwork__tags
				each item in tagList
					li
						a(href= urlRootTag + item)= item

mixin artworkCard(pieceName, imageSource, imageAlt, pieceYear, pieceMedium, pieceMaterial, pieceWidth, pieceHeight, aspectRatio, seriesName, seriesLink, ...tagList)
	//- Lightbox image and caption
	figure.ap-c-crd-artwork(
		data-src="https://res.cloudinary.com/adamska/image/upload/f_auto,q_auto:best/v1/work/grazyna-adamska-jarecka_" + imageSource + ".png"
		data-sub-html=".ap-c-crd-artwork__info"
	)
		//- Thumbnail container
		div.ap-c-crd-artwork__thumb(
			class="ap-l-ratio--" + aspectRatio
			style="background-image: url(https://res.cloudinary.com/adamska/image/upload/c_scale,e_blur:512,f_auto,q_auto:good,w_64/v1/work/grazyna-adamska-jarecka_" + imageSource + "-thumb.png);"
		)
			//- Thumbnail image
			img.ap-c-crd-artwork__image(
				src="https://res.cloudinary.com/adamska/image/upload/f_auto,q_auto:best/v1/work/grazyna-adamska-jarecka_" + imageSource + "-thumb.png"
				alt= imageAlt + " by Grazyna Adamska Jarecka"
			)
			div.ap-c-crd-artwork__icon
				+icon(iconExpand)
		//- Info and details
		figcaption.ap-c-crd-artwork__info
			//- Title and series
			div.ap-c-crd-artwork__title
				h4.ap-c-crd-artwork__piece= pieceName
				span.ap-c-crd-artwork__series
					span &ensp;&mdash;&ensp;in&nbsp;
					a(href= seriesLink)= seriesName
			//- Mediums and materials
			div.ap-c-crd-artwork__spec
				a(
					href= urlRoot + pieceYear
					title="More artworks created in" + pieceYear
				)=pieceYear
				span &nbsp;&mdash;&nbsp;
				a(
					href= urlRoot + pieceMedium
					title="More artworks made with" + pieceMedium
				)= pieceMedium
				span &nbsp;on&nbsp;
				a(
					href=urlRoot + pieceMaterial
					title="More artworks made with" + pieceMaterial
				)= pieceMaterial
				span &comma;&nbsp;
				span= pieceWidth
					| &nbsp;&times;&nbsp;
				span= pieceHeight
					| &nbsp;in
			//- Dominant colors
			ul.ap-c-crd-artwork__palette
				li
					a(href= urlRootTag + color1
					class=".ap-c-crd-artwork__palette--" + color1
					)= color1
				li
					a(href= urlRootTag + color2
					class=".ap-c-crd-artwork__palette--" + color2
					)= color2
				li
					a(href= urlRootTag + color3
					class=".ap-c-crd-artwork__palette--" + color3
					)= color3
			//- Tag list
			ul.ap-c-crd-artwork__tags
				each item in tagList
					li
						a(href= urlRootTag + item)= item


//- Lightbox swipe/drag navigation alert component mixin
mixin lightboxNavAlert
	div.ap-c-alr-lightbox
		span.ap-c-alr-lightbox__arrow.ap-c-alr-lightbox__arrow--left
			+icon(iconChevronLeft)
			+icon(iconChevronLeft)
			+icon(iconChevronLeft)
		span.ap-c-alr-lightbox__label.ap-c-alr-lightbox__label--mobile Swipe
		span.ap-c-alr-lightbox__label.ap-c-alr-lightbox__label--desktop Drag or use arrow keys
		span.ap-c-alr-lightbox__arrow.ap-c-alr-lightbox__arrow--right
			+icon(iconChevronRight)
			+icon(iconChevronRight)
			+icon(iconChevronRight)


