//- Shared components

//- Button mixin
	Define:
	Label text,
	SVG icon path,
	Link address,
	Title attribute text,
	Target attribute
mixin button(buttonText, buttonIcon, buttonLink, buttonInfo, buttonTarget)
	a.ap-c-btn-contained(
		href=buttonLink
		title=buttonInfo
		target=buttonTarget
	)
		span=buttonText
		svg(
			width="16" 
			height="16" 
			viewBox="0 0 16 16" 
			fill="none" 
			xmlns="http://www.w3.org/2000/svg"
		)
			path(
				fill-rule="evenodd" 
				clip-rule="evenodd" 
				d=buttonIcon 
				fill="black"
			)

//- SVG icon mixin
	Defines svg path
mixin icon(svgPath)
	svg(
		width="16" 
		height="16" 
		viewBox="0 0 16 16" 
		fill="none" 
		xmlns="http://www.w3.org/2000/svg"
	)
		path(
			fill-rule="evenodd" 
			clip-rule="evenodd" 
			d=svgPath 
			fill="#473793"
		)

//- Section header mixins
//- Section header with title only
mixin headerTitle(sectionTitle)
	div.ap-c-scn__header
		header.ap-c-scn__header-container
			h2.ap-c-scn__header-title
				span= sectionTitle

//- Section header with title and button
mixin headerTitleAndButton(sectionTitle, buttonText, buttonIcon, buttonLink, buttonInfo, buttonTarget)
	div.ap-c-scn__header
		header.ap-c-scn__header-container
			h2.ap-c-scn__header-title
				span= sectionTitle
			a.ap-c-btn-contained(
				href= buttonLink
				title= buttonInfo
				target= buttonTarget
			)
				span= buttonText
				svg(
					width="16" 
					height="16" 
					viewBox="0 0 16 16" 
					fill="none" 
					xmlns="http://www.w3.org/2000/svg"
				)
					path(
						fill-rule= "evenodd" 
						clip-rule= "evenodd" 
						d= buttonIcon 
						fill= "black"
					)

//- Main content outro CTA
mixin contactFooter
	div.ap-c-scn__footer
		footer.ap-c-scn__footer-container
			h3.ap-c-scn__footer-title
				p Get in touch with Grazyna by sending an email to #[a(href= email.link title= email.info) #[span= email.name]]
			+button(
				"Contact Grazyna",
				iconEmail,
				email.link,
				email.info,
				"_blank"
			)

//- Lightbox swipe/drag navigation alert component mixin
mixin lightboxNavAlert
	div.ap-c-alr-lightbox
		span.ap-c-alr-lightbox__arrow.ap-c-alr-lightbox__arrow--left
			+icon(iconChevronLeft)
			+icon(iconChevronLeft)
			+icon(iconChevronLeft)
		span.ap-c-alr-lightbox__label.ap-c-alr-lightbox__label--mobile Swipe
		span.ap-c-alr-lightbox__label.ap-c-alr-lightbox__label--desktop Drag or use arrow keys
		span.ap-c-alr-lightbox__arrow.ap-c-alr-lightbox__arrow--right
			+icon(iconChevronRight)
			+icon(iconChevronRight)
			+icon(iconChevronRight)


